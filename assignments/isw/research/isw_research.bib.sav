  keywords  = {TOP30, tomato harvesting robot, four-wheel independent steering, automatic navigation, binocular stereo vision system, obstacle avoidance, greenhouse, rank3},
  publisher = {International Journal of Agricultural and Biological Engineering},
  ranking   = {rank3},
}

